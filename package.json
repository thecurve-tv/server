{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "audit": "npm audit --audit-level=moderate --production",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "format": "npm run lint -- --fix",
    "prebuild": "rm -rf build",
    "build": "tsc",
    "build:test": "npm run build -- -p test/tsconfig.json",
    "postbuild": "cp app.yaml package*.json build && echo Build complete. You may now create a .env file in the build directory.",
    "setup-node": "set NODE_OPTIONS='--unhandled-rejections=strict'",
    "test": "ts-node --transpile-only test/run-tests",
    "start": "npm run setup-node && node .",
    "start:dev": "npm run setup-node && ts-node --transpile-only test/start-test-server dev",
    "start:test": "npm run setup-node && ts-node --transpile-only test/start-test-server test"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.18.4",
    "apollo-server-express": "^2.25.3",
    "auth0": "^2.37.0",
    "bson": "^4.6.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "graphql": "^15.8.0",
    "graphql-compose": "^9.0.6",
    "graphql-compose-mongoose": "^9.7.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "mongoose": "^5.13.14",
    "morgan": "~1.9.1",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/auth0": "^2.34.10",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.7",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "axios": "^0.24.0",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "jest-circus": "^27.4.6",
    "madge": "^5.0.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.4"
  }
}
