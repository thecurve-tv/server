{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "package": "npm run format && npm run build && cp app.yaml dist/server",
    "build": "tsc && cp package*.json src/.env dist/server",
    "lint": "prettier -c .",
    "format": "prettier --write .",
    "start": "cd src && ts-node bin/www --unhandled-rejections=strict"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.14.0",
    "apollo-server-express": "^2.24.0",
    "auth0": "^2.34.2",
    "bson": "^4.4.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "graphql": "^15.5.0",
    "graphql-compose": "^8.0.1",
    "graphql-compose-mongoose": "^9.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.2",
    "mongoose": "^5.12.7",
    "morgan": "~1.9.1",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/auth0": "^2.33.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/morgan": "^1.9.2",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
